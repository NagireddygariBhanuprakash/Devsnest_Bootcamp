#                                                        22.    PATTERM 3
# Example 1
# Input:
#     5
# Output:
#     1
#    12
#   123
#  1234
# 12345
    
#{                                                                  METHOD-1
# n=int(input())
# for i in range(n):
#     for k in range(n-i-1):
#         print(" ",end="")
#     for j in range(i+1):
#         print(j+1,end="")
#     print()
                                                                      #  METHOD_2

n=int(input())
for i in range(n):
    for j in range(n-i-1):
        print(" ",end="")
    for j in range(i+1):
        print(j+1,end="")
    print()
    #}

#     23.PATTERN-4
#     Example 1
# Input:
#     5
# Output:
# 12345
#  1234
#   123
#    12
#     1


n=int(input())
for i in range(n+1,1,-1):
    for j in range(n,i,-1):
        print(" ",end="")
    for j in range(1,i):
        print(j,end="")
    print()
   